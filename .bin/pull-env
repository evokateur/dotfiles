#!/bin/bash

echo "Running 'dotfiles pull'.."
source ~/.config/shell/functions/dotfiles.sh && dotfiles pull
echo
echo "Running 'pass git pull'.."
pass git pull
echo
echo "Running 'git pull' in ~/.config/nvim/.."
cd ~/.config/nvim && git pull
echo
echo "Running 'git pull' in ~/obsidian-vault/.."
cd ~/obsidian-vault/ && git pull
echo
echo "Done."
